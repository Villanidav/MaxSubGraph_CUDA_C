import sys

def compare_files():
    """
    Compares the contents of two files and returns True if they are equal, False otherwise.
    """
    try:
        with open("output.txt", 'r') as f1, open("outputPython.txt", 'r') as f2:
            # Compare lines one by one
            for line1, line2 in zip(f1, f2):
                if line1.strip() != line2.strip():
                    return False
                # If we reach here, all lines matched
                return True
    except FileNotFoundError:
        print(f"Error: One or both files (output, outputPython) not found.")
        return False
    except OSError as e:
        print(f"Error: An error occurred while accessing the files. ({e})")
    return False



if compare_files():
    print("The files are equal.")
else:
    print("The files are not equal.")

